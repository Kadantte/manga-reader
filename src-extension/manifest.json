{
  "key": "",
  "manifest_version": 2,
  "name": "Manga Reader",
  "description": "Extension to support usage of the browser version of Yentis' Manga Reader",
  "author": "Yentis#5218",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "cookies"
  ],
  "externally_connectable": {
    "matches": [
      "https://yentis.github.io/*",
      "http://localhost/*"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}